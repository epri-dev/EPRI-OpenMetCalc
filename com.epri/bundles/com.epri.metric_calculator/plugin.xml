<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/electric_16.png">
         <run
               class="com.epri.metric_calculator.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.epri.metric_calculator.perspectives.DataPointPerspective"
            icon="icons/data_point_16.png"
            id="com.epri.metric_calculator.perspectives.DataPointPerspective"
            name="Data Point">
      </perspective>
      <perspective
            class="com.epri.metric_calculator.perspectives.MetricPerspective"
            icon="icons/metric_16.png"
            id="com.epri.metric_calculator.perspectives.MetricPerspective"
            name="Metric">
      </perspective>
      <perspective
            class="com.epri.metric_calculator.perspectives.DashboardPerspective"
            icon="icons/Dashboard_16px.png"
            id="com.epri.metric_calculator.perspectives.DashboardPerspective"
            name="Dashboard">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            class="com.epri.metric_calculator.views.datapoint.DataPointTableView"
            id="com.epri.metric_calculator.views.datapoint.DataPointTableView"
            name="Data Point Table"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="com.epri.metric_calculator.views.datapoint.DataPointScopeView"
            id="com.epri.metric_calculator.views.datapoint.DataPointScopeView"
            name="Data Point Scope"
            restorable="true">
      </view>
      <view
            class="com.epri.metric_calculator.views.datapoint.ImportConsoleView"
            icon="icons/monitor_16.png"
            id="com.epri.metric_calculator.views.datapoint.ImportConsoleView"
            name="Import console"
            restorable="true">
      </view>
      <view
            class="com.epri.metric_calculator.views.metric.CalculationConsoleView"
            icon="icons/monitor_16.png"
            id="com.epri.metric_calculator.views.metric.CalculationConsoleView"
            name="Calculation console"
            restorable="true">
      </view>
      <view
            class="com.epri.metric_calculator.views.metric.MetricView"
            id="com.epri.metric_calculator.views.metric.MetricView"
            name="Calculate Metric"
            restorable="true">
      </view>
      <view
            class="com.epri.metric_calculator.views.dashboard.DashboardView"
            id="com.epri.metric_calculator.views.dashboard.DashboardView"
            name="Dashboard"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.epri.metric_calculator.command.Create"
            name="Create">
      </command>
            <command
            id="com.epri.metric_calculator.command.Open"
            name="Open">
      </command>
      <command
            id="com.epri.metric_calculator.command.Save"
            name="Save">
      </command>
      <command
            id="com.epri.metric_calculator.command.SaveAs"
            name="Save As">
      </command>
      <command
            id="com.epri.metric_calculator.command.ImportData"
            name="Import Data">
      </command>
      <command
            id="com.epri.metric_calculator.command.ExportData"
            name="Export Data">
      </command>
      <command
            id="com.epri.metric_calculator.command.ExportToText"
            name="Export To Text">
      </command>
      <command
            defaultHandler="com.epri.metric_calculator.handler.ReCalcurateHandler"
            id="com.epri.metric_calculator.actions.recalculation.RecalculationAction"
            name="Re-Calcurate">
      </command>
      <command
            id="com.epri.metric_calculator.command.ImportMetric"
            name="Import Metric">
      </command>
      <command
            id="com.epri.metric_calculator.command.ExportMetric"
            name="Export Metric">
      </command>
      <command
            id="com.epri.metric_calculator.command.ExportToExcel"
            name="Export Metric to Excel">
      </command>
      <command
            id="com.epri.metric_calculator.command.SnapshotAction"
            name="Snapshot">
      </command>
      <command
            id="com.epri.metric_calculator.command.DataPointPerspectiveAction"
            name="com.epri.metric_calculator.command.DataPointPerspectiveAction">
      </command>
      <command
            id="com.epri.metric_calculator.perspectives.MetricPerspective"
            name="com.epri.metric_calculator.perspectives.MetricPerspective">
      </command>
      <command
            id="com.epri.metric_calculator.actions.file.SettingAction"
            name="Setting">
      </command>
      <command
            id="com.epri.metric_calculator.actions.export.GenerateKeyAction"
            name="Generate Key">
      </command>
      <command
            id="com.epri.metric_calculator.actions.export.ImportReferenceValueAction"
            name="Import Reference Value">
      </command>
      <command
            id="com.epri.metric_calculator.actions.export.EncryptDataPointAction"
            name="Export Encrypted CSV">
      </command>
      <command
            id="com.epri.metric_calculator.actions.export.DecryptDataPointAction"
            name="Import Encrypted CSV">
      </command>
      <command
            id="com.epri.metric_calculator.actions.metricshub.MetricsHubUploadAction"
            name="Upload Data to Metrics Hub">
      </command>
      <command
            id="com.epri.metric_calculator.actions.metricshubsetting.MetricsHubSettingAction"
            name="Metrics Hub Setting">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            id="com.epri.metric_calculator.scheme"
            name="myscheme">
      </scheme>
      <key
            commandId="com.epri.metric_calculator.command.Save"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="com.epri.metric_calculator.scheme"
            sequence="CTRL+S">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="com.epri.metric_calculator.splashHandlers.InteractiveSplashHandler"
            id="com.epri.metric_calculator.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            productId="com.epri.metric_calculator.product"
            splashId="com.epri.metric_calculator.splashHandlers.interactive">
      </splashHandlerProductBinding>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.epri.metric_calculator.application"
            name="EPRI OpenMetCalc">
         <property
               name="windowImages"
               value="icons/window_16.png,icons/window_32.png,icons/window_48.png,icons/window_64.png">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="appName"
               value="EPRI OpenMetCalc">
         </property>
         <property
